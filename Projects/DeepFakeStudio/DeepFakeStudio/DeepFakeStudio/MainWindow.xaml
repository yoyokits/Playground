<Window
    x:Class="DeepFakeStudio.MainWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:viewmodels="clr-namespace:DeepFakeStudio.ViewModels"
    xmlns:views="clr-namespace:DeepFakeStudio.Views"
    Title="Deep Fake Studio"
    Width="800"
    Height="450"
    mc:Ignorable="d">
    <Window.Resources>
        <viewmodels:DeepFakeStudioViewModel x:Key="DeepFakeStudioViewModel" />
    </Window.Resources>
    <Grid>
        <views:DeepFakeStudioView DataContext="{StaticResource DeepFakeStudioViewModel}" />
    </Grid>
</Window>